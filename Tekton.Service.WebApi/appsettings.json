{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TektonDBConnection": "server=(local); database=Tekton; User Id=sa; Password=root; trustServerCertificate=true;"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Health Checks API",
        "Uri": "/health"
      }
    ],
    "EvaluationTimeInSeconds": 500
  },
  "RateLimiting": {
    "PermitLimit": 20,
    "Window": 30,
    "QueueLimit": 2
  }
}
